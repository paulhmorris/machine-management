{
  "name": "machine-management-c7b1",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:remix": "remix build",
    "build:server": "esbuild --platform=node --format=cjs ./server.ts --outdir=build --bundle --external:fsevents",
    "dev": "run-p dev:*",
    "dev:server": "cross-env NODE_ENV=development npm run build:server -- --watch",
    "dev:remix": "remix dev --manual -c \"node --require ./mocks ./build/server.js\"",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "prisma generate && prisma migrate deploy && prisma db seed",
    "start": "cross-env NODE_ENV=production node ./build/server.js",
    "start:mocks": "cross-env NODE_ENV=production node --require ./mocks --require dotenv/config ./build/server.js",
    "test": "vitest",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 \"npx cypress open\"",
    "pretest:e2e:run": "npm run build",
    "prisma:reset-db": "prisma db push --force-reset && prisma db seed",
    "test:e2e:run": "cross-env PORT=8811 start-server-and-test start:mocks http://localhost:8811 \"npx cypress run\"",
    "typecheck": "tsc && tsc -p cypress",
    "upgrade-remix": "ncu @remix-run/*",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run"
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build",
    "/postgres-data"
  ],
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@isaacs/express-prometheus-middleware": "^1.2.1",
    "@prisma/client": "^5.3.1",
    "@remix-run/express": "^2.0.1",
    "@remix-run/node": "^2.0.1",
    "@remix-run/react": "^2.0.1",
    "@remix-run/server-runtime": "^2.0.1",
    "@tabler/icons-react": "^2.35.0",
    "@tailwindcss/forms": "^0.5.6",
    "@types/nodemailer": "^6.4.11",
    "autoprefixer": "^10.4.16",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.10",
    "express": "^4.18.2",
    "isbot": "^3.7.0",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.5",
    "postcss": "^8.4.30",
    "prom-client": "^14.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "remix-typedjson": "^0.2.2",
    "source-map-support": "^0.5.21",
    "spin-delay": "^1.2.0",
    "tiny-invariant": "^1.3.1",
    "usehooks-ts": "^2.9.1",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.1.0",
    "@remix-run/dev": "^2.0.1",
    "@remix-run/eslint-config": "^2.0.1",
    "@testing-library/cypress": "^10.0.1",
    "@testing-library/dom": "^9.3.3",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/bcryptjs": "^2.4.4",
    "@types/compression": "^1.7.3",
    "@types/eslint": "^8.44.3",
    "@types/express": "^4.17.18",
    "@types/morgan": "^1.9.5",
    "@types/node": "^20.7.0",
    "@types/react": "^18.2.23",
    "@types/react-dom": "^18.2.7",
    "@types/source-map-support": "^0.5.8",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "@vitejs/plugin-react": "^4.1.0",
    "@vitest/coverage-c8": "^0.29.5",
    "c8": "^8.0.1",
    "cookie": "^0.5.0",
    "cypress": "^13.2.0",
    "dotenv": "^16.3.1",
    "esbuild": "^0.19.3",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-cypress": "^2.15.1",
    "happy-dom": "^12.2.0",
    "msw": "^1.3.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "prisma": "^5.3.1",
    "start-server-and-test": "^2.0.1",
    "tailwindcss": "^3.3.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.5"
  },
  "engines": {
    "node": "18.18.0"
  },
  "prisma": {
    "seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
  },
  "volta": {
    "node": "18.18.0"
  }
}
